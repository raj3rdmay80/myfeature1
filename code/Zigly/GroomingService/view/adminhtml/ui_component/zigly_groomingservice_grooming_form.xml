		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider name="grooming_form_data_source" class="Zigly\GroomingService\Model\Grooming\DataProvider">
			<settings>
				<requestFieldName>entity_id</requestFieldName>
				<primaryFieldName>entity_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
