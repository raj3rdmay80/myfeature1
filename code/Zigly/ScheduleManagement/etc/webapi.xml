<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route url="/V1/fetch/schedule" method="POST">
		<service class="Zigly\ScheduleManagement\Api\FetchVendorScheduleManagementInterface" method="getFetchVendorSchedule"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	<route url="/V1/save/schedule" method="POST">
		<service class="Zigly\ScheduleManagement\Api\SaveVendorScheduleManagementInterface" method="postSaveVendorSchedule"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	<route url="/V1/zigly-schedulemanagement/schedulemanagement" method="POST">
		<service class="Zigly\ScheduleManagement\Api\ScheduleManagementRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Zigly_ScheduleManagement::ScheduleManagement_save"/>
		</resources>
	</route>
	<route url="/V1/zigly-schedulemanagement/schedulemanagement/search" method="GET">
		<service class="Zigly\ScheduleManagement\Api\ScheduleManagementRepositoryInterface" method="getList"/>
		<resources>
			<resource ref="Zigly_ScheduleManagement::ScheduleManagement_view"/>
		</resources>
	</route>
	<route url="/V1/zigly-schedulemanagement/schedulemanagement/:schedulemanagementId" method="GET">
		<service class="Zigly\ScheduleManagement\Api\ScheduleManagementRepositoryInterface" method="get"/>
		<resources>
			<resource ref="Zigly_ScheduleManagement::ScheduleManagement_view"/>
		</resources>
	</route>
	<route url="/V1/zigly-schedulemanagement/schedulemanagement/:schedulemanagementId" method="PUT">
		<service class="Zigly\ScheduleManagement\Api\ScheduleManagementRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Zigly_ScheduleManagement::ScheduleManagement_update"/>
		</resources>
	</route>
	<route url="/V1/zigly-schedulemanagement/schedulemanagement/:schedulemanagementId" method="DELETE">
		<service class="Zigly\ScheduleManagement\Api\ScheduleManagementRepositoryInterface" method="deleteById"/>
		<resources>
			<resource ref="Zigly_ScheduleManagement::ScheduleManagement_delete"/>
		</resources>
	</route>
</routes>
