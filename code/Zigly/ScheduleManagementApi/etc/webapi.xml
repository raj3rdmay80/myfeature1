<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route url="/V1/zigly-schedulemanagementapi/schedule" method="GET">
		<service class="Zigly\ScheduleManagementApi\Api\ScheduleManagementInterface" method="getSchedule"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
        <data>
	       <parameter name="date" force="true">%date%</parameter>
	       <parameter name="pincode" force="true">%pincode%</parameter>
	       <parameter name="id" force="true">%customer_id%</parameter>
	   </data>
	</route>
