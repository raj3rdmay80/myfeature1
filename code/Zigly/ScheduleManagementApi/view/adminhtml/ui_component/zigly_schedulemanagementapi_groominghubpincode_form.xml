<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">zigly_schedulemanagementapi_groominghubpincode_form.groominghubpincode_form_data_source</item>
		</item>
		<item name="label" xsi:type="string" translate="true">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button name="back" class="Zigly\ScheduleManagementApi\Block\Adminhtml\GroomingHubPincode\Edit\BackButton"/>
			<button name="delete" class="Zigly\ScheduleManagementApi\Block\Adminhtml\GroomingHubPincode\Edit\DeleteButton"/>
			<button name="save" class="Zigly\ScheduleManagementApi\Block\Adminhtml\GroomingHubPincode\Edit\SaveButton"/>
			<button name="save_and_continue" class="Zigly\ScheduleManagementApi\Block\Adminhtml\GroomingHubPincode\Edit\SaveAndContinueButton"/>
		</buttons>
		<namespace>zigly_schedulemanagementapi_groominghubpincode_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>zigly_schedulemanagementapi_groominghubpincode_form.groominghubpincode_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="groominghubpincode_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider name="groominghubpincode_form_data_source" class="Zigly\ScheduleManagementApi\Model\GroomingHubPincode\DataProvider">
			<settings>
				<requestFieldName>pincode_id</requestFieldName>
				<primaryFieldName>pincode_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field name="hub_id" formElement="select" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">GroomingHubPincode</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">Hub Name</label>
				<dataScope>hub_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
			<formElements>
			    <select>
			        <settings>
			            <options class="Zigly\ScheduleManagementApi\Model\GroomingHub\Source\Huboption"/>
			            <caption translate="true">-- Please Select --</caption>
			        </settings>
			    </select>
			</formElements>
		</field>
		<field name="pincode" formElement="select" sortOrder="30">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">GroomingHubPincode</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">pincode</label>
				<dataScope>pincode</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
			<formElements>
			    <select>
			        <settings>
			            <options class="Zigly\ScheduleManagementApi\Model\GroomingHub\Source\Pincode"/>
			            <caption translate="true">-- Please Select --</caption>
			        </settings>
			    </select>
			</formElements>
		</field>
		<field name="status" formElement="select" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">GroomingHubPincode</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">status</label>
				<dataScope>status</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
			<formElements>
			  	<select>
					<settings>
						<options class="Zigly\ScheduleManagementApi\Model\Status"/>
					</settings>
				</select>
			</formElements>
		</field>
	</fieldset>
</form>
