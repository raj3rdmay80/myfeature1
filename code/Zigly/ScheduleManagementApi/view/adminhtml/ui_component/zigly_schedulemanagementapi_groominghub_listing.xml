<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">zigly_schedulemanagementapi_groominghub_listing.zigly_schedulemanagementapi_groominghub_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>zigly_schedulemanagementapi_groominghub_columns</spinner>
		<deps>
			<dep>zigly_schedulemanagementapi_groominghub_listing.zigly_schedulemanagementapi_groominghub_listing_data_source</dep>
		</deps>
		<buttons>
			<button name="add">
				<url path="*/*/new"/>
				<class>primary</class>
				<label translate="true">Add New Hub</label>
			</button>
		</buttons>
	</settings>
	<dataSource name="zigly_schedulemanagementapi_groominghub_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">hub_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Zigly_ScheduleManagementApi::GroomingHub</aclResource>
		<dataProvider name="zigly_schedulemanagementapi_groominghub_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>hub_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="zigly_schedulemanagementapi_groominghub_columns">
		<settings>
			<editorConfig>
				<param name="selectProvider" xsi:type="string">zigly_schedulemanagementapi_groominghub_listing.zigly_schedulemanagementapi_groominghub_listing.zigly_schedulemanagementapi_groominghub_columns.ids</param>
				<param name="enabled" xsi:type="boolean">true</param>
				<param name="indexField" xsi:type="string">hub_id</param>
				<param name="clientConfig" xsi:type="array">
					<item name="saveUrl" xsi:type="url" path="zigly_schedulemanagementapi/GroomingHub/inlineEdit"/>
					<item name="validateBeforeSave" xsi:type="boolean">false</item>
				</param>
			</editorConfig>
			<childDefaults>
				<param name="fieldAction" xsi:type="array">
					<item name="provider" xsi:type="string">zigly_schedulemanagementapi_groominghub_listing.zigly_schedulemanagementapi_groominghub_listing.zigly_schedulemanagementapi_groominghub_columns_editor</item>
					<item name="target" xsi:type="string">startEdit</item>
					<item name="params" xsi:type="array">
						<item name="0" xsi:type="string">${ $.$data.rowIndex }</item>
						<item name="1" xsi:type="boolean">true</item>
					</item>
				</param>
			</childDefaults>
		</settings>
		<selectionsColumn name="ids">
			<settings>
				<indexField>hub_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="hub_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="hub_name">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">name</label>
			</settings>
		</column>
		<!-- <column name="status">
			<settings>
				<filter>text</filter>
				<label translate="true">Status</label>
			</settings>
		</column> -->
		<column name="status">
			<argument name="data" xsi:type="array">
				<item name="options" xsi:type="object">Zigly\ScheduleManagementApi\Ui\Component\Listing\Column\Status</item>
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">select</item>
					<item name="label" xsi:type="string" translate="true">Is Active</item>
					<item name="component" xsi:type="string">Magento_Ui/js/grid/columns/select</item>
					<item name="dataType" xsi:type="string">select</item>
					<item name="editor" xsi:type="array">
						<item name="editorType" xsi:type="string">select</item>
						<item name="validation" xsi:type="array">
							<item name="required-entry" xsi:type="boolean">false</item>
						</item>
					</item>
				</item>
			</argument>
		</column>
		<column name="created_date">
			<settings>
				<filter>text</filter>
				<label translate="true">Created Date</label>
			</settings>
		</column>
		<column name="updated_date">
			<settings>
				<filter>text</filter>
				<label translate="true">Updated Date</label>
			</settings>
		</column>
		<actionsColumn name="actions" class="Zigly\ScheduleManagementApi\Ui\Component\Listing\Column\GroomingHubActions">
			<settings>
				<indexField>hub_id</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>107</resizeDefaultWidth>
			</settings>
		</actionsColumn>
	</columns>
</listing>
