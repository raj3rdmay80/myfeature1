<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">zigly_schedulemanagementapi_groomingslottable_listing.zigly_schedulemanagementapi_groomingslottable_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>zigly_schedulemanagementapi_groomingslottable_columns</spinner>
		<deps>
			<dep>zigly_schedulemanagementapi_groomingslottable_listing.zigly_schedulemanagementapi_groomingslottable_listing_data_source</dep>
		</deps>
		<buttons>
			<button name="add">
				<url path="*/*/new"/>
				<class>primary</class>
				<label translate="true">Add new Time Slot</label>
			</button>
		</buttons>
	</settings>
	<dataSource name="zigly_schedulemanagementapi_groomingslottable_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">slot_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Zigly_ScheduleManagementApi::GroomingSlotTable</aclResource>
		<dataProvider name="zigly_schedulemanagementapi_groomingslottable_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>slot_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="zigly_schedulemanagementapi_groomingslottable_columns">
		<settings>
			<editorConfig>
				<param name="selectProvider" xsi:type="string">zigly_schedulemanagementapi_groomingslottable_listing.zigly_schedulemanagementapi_groomingslottable_listing.zigly_schedulemanagementapi_groomingslottable_columns.ids</param>
				<param name="enabled" xsi:type="boolean">true</param>
				<param name="indexField" xsi:type="string">slot_id</param>
				<param name="clientConfig" xsi:type="array">
					<item name="saveUrl" xsi:type="url" path="zigly_schedulemanagementapi/GroomingSlotTable/inlineEdit"/>
					<item name="validateBeforeSave" xsi:type="boolean">false</item>
				</param>
			</editorConfig>
			<childDefaults>
				<param name="fieldAction" xsi:type="array">
					<item name="provider" xsi:type="string">zigly_schedulemanagementapi_groomingslottable_listing.zigly_schedulemanagementapi_groomingslottable_listing.zigly_schedulemanagementapi_groomingslottable_columns_editor</item>
					<item name="target" xsi:type="string">startEdit</item>
					<item name="params" xsi:type="array">
						<item name="0" xsi:type="string">${ $.$data.rowIndex }</item>
						<item name="1" xsi:type="boolean">true</item>
					</item>
				</param>
			</childDefaults>
		</settings>
		<selectionsColumn name="ids">
			<settings>
				<indexField>slot_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="slot_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="hub_id" class="Zigly\ScheduleManagementApi\Model\GroomingHub\Source\Hubname">
			<settings>
				<filter>text</filter>
				<label translate="true">Hub Name</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="date">
			<settings>
				<filter>text</filter>
				<label translate="true">Date</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="slot_start_time">
			<settings>
				<filter>text</filter>
				<label translate="true">Start Time</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="slot_end_time">
			<settings>
				<filter>text</filter>
				<label translate="true">End Time</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="allowed_booking">
			<argument name="data" xsi:type="array">
				<item name="options" xsi:type="object">Zigly\ScheduleManagementApi\Ui\Component\Listing\Column\Status</item>
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">select</item>
					<item name="label" xsi:type="string" translate="true">Allowed Booking</item>
					<item name="component" xsi:type="string">Magento_Ui/js/grid/columns/select</item>
					<item name="dataType" xsi:type="string">select</item>
					<item name="editor" xsi:type="array">
						<item name="editorType" xsi:type="string">select</item>
						<item name="validation" xsi:type="array">
							<item name="required-entry" xsi:type="boolean">false</item>
						</item>
					</item>
				</item>
			</argument>
		</column>
		<column name="status">
			<argument name="data" xsi:type="array">
				<item name="options" xsi:type="object">Zigly\ScheduleManagementApi\Ui\Component\Listing\Column\Status</item>
				<item name="config" xsi:type="array">
					<item name="filter" xsi:type="string">select</item>
					<item name="label" xsi:type="string" translate="true">Is Active</item>
					<item name="component" xsi:type="string">Magento_Ui/js/grid/columns/select</item>
					<item name="dataType" xsi:type="string">select</item>
					<item name="editor" xsi:type="array">
						<item name="editorType" xsi:type="string">select</item>
						<item name="validation" xsi:type="array">
							<item name="required-entry" xsi:type="boolean">false</item>
						</item>
					</item>
				</item>
			</argument>
		</column>
		<column name="created_at">
			<settings>
				<filter>text</filter>
				<label translate="true">Create At</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="updated_at">
			<settings>
				<filter>text</filter>
				<label translate="true">Upated At</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<actionsColumn name="actions" class="Zigly\ScheduleManagementApi\Ui\Component\Listing\Column\GroomingSlotTableActions">
			<settings>
				<indexField>slot_id</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>107</resizeDefaultWidth>
			</settings>
		</actionsColumn>
	</columns>
</listing>
