<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route url="/V1/zigly-mobilehome/mobilehome" method="GET">
		<service class="Zigly\Mobilehome\Api\MobilehomeManagementInterface" method="getMobilehome"/>
		<resources>
			<resource ref="anonymous"/>
		</resources>
	</route>
	<route url="/V1/zigly-mobilehome/mobilehome" method="POST">
		<service class="Zigly\Mobilehome\Api\MobilehomeRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Zigly_Mobilehome::mobilehome_save"/>
		</resources>
	</route>
	<route url="/V1/zigly-mobilehome/mobilehome/search" method="GET">
		<service class="Zigly\Mobilehome\Api\MobilehomeRepositoryInterface" method="getList"/>
		<resources>
			<resource ref="Zigly_Mobilehome::mobilehome_view"/>
		</resources>
	</route>
	<route url="/V1/zigly-mobilehome/mobilehome/:mobilehomeId" method="GET">
		<service class="Zigly\Mobilehome\Api\MobilehomeRepositoryInterface" method="get"/>
		<resources>
			<resource ref="Zigly_Mobilehome::mobilehome_view"/>
		</resources>
	</route>
	<route url="/V1/zigly-mobilehome/mobilehome/:mobilehomeId" method="PUT">
		<service class="Zigly\Mobilehome\Api\MobilehomeRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Zigly_Mobilehome::mobilehome_update"/>
		</resources>
	</route>
	<route url="/V1/zigly-mobilehome/mobilehome/:mobilehomeId" method="DELETE">
		<service class="Zigly\Mobilehome\Api\MobilehomeRepositoryInterface" method="deleteById"/>
		<resources>
			<resource ref="Zigly_Mobilehome::mobilehome_delete"/>
		</resources>
	</route>
</routes>
