<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">zigly_mobilehome_mobilehome_listing.zigly_mobilehome_mobilehome_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>zigly_mobilehome_mobilehome_columns</spinner>
		<deps>
			<dep>zigly_mobilehome_mobilehome_listing.zigly_mobilehome_mobilehome_listing_data_source</dep>
		</deps>
		<buttons>
			<button name="add">
				<url path="*/*/new"/>
				<class>primary</class>
				<label translate="true">Add new mobilehome</label>
			</button>
		</buttons>
	</settings>
	<dataSource name="zigly_mobilehome_mobilehome_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">mobilehome_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Zigly_Mobilehome::mobilehome</aclResource>
		<dataProvider name="zigly_mobilehome_mobilehome_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>mobilehome_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="zigly_mobilehome_mobilehome_columns">
		<settings>
			<editorConfig>
				<param name="selectProvider" xsi:type="string">zigly_mobilehome_mobilehome_listing.zigly_mobilehome_mobilehome_listing.zigly_mobilehome_mobilehome_columns.ids</param>
				<param name="enabled" xsi:type="boolean">true</param>
				<param name="indexField" xsi:type="string">mobilehome_id</param>
				<param name="clientConfig" xsi:type="array">
					<item name="saveUrl" xsi:type="url" path="zigly_mobilehome/mobilehome/inlineEdit"/>
					<item name="validateBeforeSave" xsi:type="boolean">false</item>
				</param>
			</editorConfig>
			<childDefaults>
				<param name="fieldAction" xsi:type="array">
					<item name="provider" xsi:type="string">zigly_mobilehome_mobilehome_listing.zigly_mobilehome_mobilehome_listing.zigly_mobilehome_mobilehome_columns_editor</item>
					<item name="target" xsi:type="string">startEdit</item>
					<item name="params" xsi:type="array">
						<item name="0" xsi:type="string">${ $.$data.rowIndex }</item>
						<item name="1" xsi:type="boolean">true</item>
					</item>
				</param>
			</childDefaults>
		</settings>
		<selectionsColumn name="ids">
			<settings>
				<indexField>mobilehome_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="mobilehome_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="name">
			<settings>
				<filter>text</filter>
				<label translate="true">name</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="image" class="Zigly\Mobilehome\Ui\Component\Listing\Column\Thumbnail">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="component" xsi:type="string">Magento_Ui/js/grid/columns/thumbnail</item>
					<item name="sortable" xsi:type="boolean">false</item>
					<item name="altField" xsi:type="string">image</item>
					<item name="has_preview" xsi:type="string">1</item>
					<item name="label" xsi:type="string" translate="true">Banner Image</item>
					<item name="sortOrder" xsi:type="number">20</item>
				</item>
			</argument>
		</column>
		<column name="url_key">
		    <settings>
		    	<filter>text</filter>
		    	<label translate="true">Url Key</label>
		    	<editor>
		    		<editorType>text</editorType>
		    		<validation>
		    			<rule name="required-entry" xsi:type="boolean">false</rule>
		    		</validation>
		    	</editor>
		    </settings>
		</column>
		<column name="isclickable">
		    <settings>
		    	<filter>text</filter>
		    	<label translate="true">Is Clickable</label>
		    	<editor>
		    		<editorType>text</editorType>
		    		<validation>
		    			<rule name="required-entry" xsi:type="boolean">false</rule>
		    		</validation>
		    	</editor>
		    </settings>
		</column>
		<column name="type">
		    <argument name="data" xsi:type="array">
		        <item name="options" xsi:type="object">Zigly\Mobilehome\Model\Config\Source\Type</item>
		        <item name="config" xsi:type="array">
		            <item name="filter" xsi:type="string">select</item>
		            <item name="label" xsi:type="string" translate="true">Type</item>
		            <item name="component" xsi:type="string">Magento_Ui/js/grid/columns/select</item>
		            <item name="dataType" xsi:type="string">select</item>
		            <item name="sortOrder" xsi:type="number">60</item>
		        </item>
		    </argument>
		</column>
		<column name="status">
		    <argument name="data" xsi:type="array">
		        <item name="options" xsi:type="object">Magento\Config\Model\Config\Source\Enabledisable</item>
		        <item name="config" xsi:type="array">
		            <item name="editor" xsi:type="string">select</item>
		            <item name="filter" xsi:type="string">select</item>
		            <item name="label" xsi:type="string" translate="true">Status</item>
		            <item name="component" xsi:type="string">Magento_Ui/js/grid/columns/select</item>
		            <item name="dataType" xsi:type="string">select</item>
		            <item name="sortOrder" xsi:type="number">70</item>
		        </item>
		    </argument>
		</column>
		<column name="position">
		    <settings>
		    	<filter>text</filter>
		    	<label translate="true">Position</label>
		    	<editor>
		    		<editorType>text</editorType>
		    		<validation>
		    			<rule name="required-entry" xsi:type="boolean">false</rule>
		    		</validation>
		    	</editor>
		    </settings>
		</column>
		<column name="updated_at" class="Magento\Ui\Component\Listing\Columns\Date">
		    <argument name="data" xsi:type="array">
		        <item name="config" xsi:type="array">
		            <item name="component" xsi:type="string">Magento_Ui/js/grid/columns/date</item>
		            <item name="dataType" xsi:type="string">date</item>
		            <item name="label" xsi:type="string" translate="true">Modified</item>
		            <item name="sortOrder" xsi:type="number">90</item>
		        </item>
		    </argument>
		</column>
		<actionsColumn name="actions" class="Zigly\Mobilehome\Ui\Component\Listing\Column\MobilehomeActions">
			<settings>
				<indexField>mobilehome_id</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>310</resizeDefaultWidth>
			</settings>
		</actionsColumn>
	</columns>
</listing>
