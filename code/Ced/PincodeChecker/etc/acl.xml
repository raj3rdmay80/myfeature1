<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                 <resource id="Ced_PincodeChecker::pincodechecker" title="Pincode Checker" sortOrder="30">
                    <resource id="Ced_PincodeChecker::manage_pincodes" title="Manage Pincodes" sortOrder="10">
                        <resource id="Ced_PincodeChecker::managepincodes" title="Manage Pincodes" sortOrder="10" />
                    </resource>
                    <resource id="Ced_PincodeChecker::config_settings" title="Configuration Settings" sortOrder="20">
                        <resource id="Ced_PincodeChecker::configsettings" title="Configuration Settings" sortOrder="10" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Ced_PincodeChecker::cedcore" title="CedCommerce Installed Extensions" />
                            <resource id="Ced_PincodeChecker::pincode" title="Pincode Checker" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>