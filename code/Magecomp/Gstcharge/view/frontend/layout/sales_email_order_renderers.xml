<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" label="Email Creditmemo Items List" design_abstraction="custom">
    <body>
        <referenceBlock name="sales.email.order.renderers">
            <block class="Magento\Sales\Block\Order\Email\Items\Order\DefaultOrder" name="sales.email.order.renderers.default" as="default" template="Magecomp_Gstcharge::email/items/order/default.phtml"/>
             <action method="setChild">
				   <argument name="alias" xsi:type="string">default</argument>
				   <argument name="block" xsi:type="string">sales.email.order.renderers.default</argument>
			 </action>
	   <block class="Magento\Sales\Block\Order\Email\Items\Order\DefaultOrder" name="sales.email.order.renderers.grouped" as="grouped" template="Magecomp_Gstcharge::email/items/order/default.phtml"/>
             <action method="setChild">
				   <argument name="alias" xsi:type="string">grouped</argument>
				   <argument name="block" xsi:type="string">sales.email.order.renderers.grouped</argument>
			 </action>
        <block class="Magento\Sales\Block\Order\Email\Items\Order\DefaultOrder" name="sales.email.order.renderers.downloadable" as="downloadable" template="Magecomp_Gstcharge::email/items/order/default.phtml"/>
            <action method="setChild">
                <argument name="alias" xsi:type="string">downloadable</argument>
                <argument name="block" xsi:type="string">sales.email.order.renderers.downloadable</argument>
            </action>
	   <block class="Magento\Sales\Block\Order\Email\Items\Order\DefaultOrder" name="sales.email.order.renderers.bundle" as="bundle" template="Magecomp_Gstcharge::email/items/order/default.phtml"/>
             <action method="setChild">
				   <argument name="alias" xsi:type="string">bundle</argument>
				   <argument name="block" xsi:type="string">sales.email.order.renderers.bundle</argument>
			 </action>
        </referenceBlock>
    </body>
</page>
