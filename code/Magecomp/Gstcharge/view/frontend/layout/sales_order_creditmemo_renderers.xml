<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="sales.order.creditmemo.renderers">
            <block class="Magento\Sales\Block\Order\Item\Renderer\DefaultRenderer" as="default" template="Magecomp_Gstcharge::order/creditmemo/items/renderer/default.phtml" name="gst_creditmemo_view_renderer"/>
			 <action method="setChild">
				   <argument name="alias" xsi:type="string">default</argument>
				   <argument name="block" xsi:type="string">gst_creditmemo_view_renderer</argument>
			 </action>
            <block class="Magento\Sales\Block\Order\Item\Renderer\DefaultRenderer" as="grouped" template="Magecomp_Gstcharge::order/creditmemo/items/renderer/default.phtml" name="gst_creditmemo_view_renderer_grouped"/>
            <action method="setChild">
                <argument name="alias" xsi:type="string">grouped</argument>
                <argument name="block" xsi:type="string">gst_creditmemo_view_renderer_grouped</argument>
            </action>
            <block class="Magento\Sales\Block\Order\Item\Renderer\DefaultRenderer" as="downloadable" template="Magecomp_Gstcharge::order/creditmemo/items/renderer/default.phtml" name="gst_creditmemo_view_renderer_downloadable"/>
            <action method="setChild">
                <argument name="alias" xsi:type="string">downloadable</argument>
                <argument name="block" xsi:type="string">gst_creditmemo_view_renderer_downloadable</argument>
            </action>
            <block class="Magento\Bundle\Block\Sales\Order\Items\Renderer" name="sales.order.creditmemo.renderers.bundle" as="bundle" template="Magecomp_Gstcharge::order/creditmemo/items/renderer/bundle.phtml"/>
        </referenceBlock>

    </body>
</page>
